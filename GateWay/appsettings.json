{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "a70c62f0-2050-4636-b193-a6be6b972e03",
    "Issuer": "ERPSystem",
    "Audience": "ERPUser",
    "ExpiryMinutes": 120
  },
  "ReverseProxy": {
    "Routes": {
      "default-route": {
        "ClusterId": "OrganisationSetup-cluster",
        "Match": {
          "Path": "/"
        },
        "Transforms": [
          {
            "PathSet": "/OrganisationSetup/ApplicationConfiguration/COMAuthentication/Login"
          }
        ]
      },
      "OrganisationSetup-route": {
        "ClusterId": "OrganisationSetup-cluster",
        "Match": {
          "Path": "/OrganisationSetup/{**catch-all}"
        }
      },
      "IMSClothHouse-route": {
        "ClusterId": "IMSClothHouse-cluster",
        "Match": {
          "Path": "/IMSClothHouse/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "OrganisationSetup-cluster": {
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        },
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:5101/"
          }
        }
      },
      "IMSClothHouse-cluster": {
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        },
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:5202/"
          }
        }
      }
    }
  }
}